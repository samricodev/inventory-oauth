{
  "server": {
    "port": 3081,
    "host": "localhost"
  },
  "mongodb": {
    "url": "${MONGODB_URI}",
    "options": {
      "useNewUrlParser": true,
      "useUnifiedTopology": true,
      "useCreateIndex": true,
      "useFindAndModify": false
    }
  },
  "cors": {
    "enabled": true,
    "origin": [
      "https://www.inventoryapp.dev",
      "https://inventory-dashboard-rho.vercel.app"
    ],
    "methods": "GET,HEAD,PUT,PATCH,POST,DELETE",
    "preflightContinue": false,
    "optionsSuccessStatus": 204
  },
  "tokenLoginExpires": 10800
}